 },\n  computed: {\n    ...mapGetters(['language']),\n    ids() {\n      return Math.random() * 100000;\n    },\n  },\n  mounted: function () {\n    this.$nextTick(() => {\n      this.drawLine()\n    });\n  },\n  \n  methods: {\n    drawLine() {\n      this.dataArr = this.data.length <= 0 ? [{ value: \"\", name: this.$t('dashboard.noData') }] : this.data;\n      let myChart = this.$echarts.init(document.getElementById(this.ids));\n      let option = {\n        title: {\n          text: this.title,\n          left: \"left\",\n        },\n        tooltip: {\n          trigger: \"item\",\n          formatter: \"{a} <br/>{b}: {c} ({d}%)\",\n        },\n        legend: {\n          orient: \"vertical\",\n          right: 20,\n          top: 50,\n          bottom: 20,\n          backgroundColor: \"#FAFAFA\",\n        },\n        label: {\n          formatter: \"{d}%\",\n          backgroundColor: \"#000\",\n          position: \"top\",\n          padding: 3,\n          color: \"#fff\",\n          borderWidth: 1,\n          borderRadius: 4,\n          rich: {\n            a: {\n              color: \"#6E7079\",\n              lineHeight: 22,\n              align: \"center\",\n            },\n            hr: {\n              borderColor: \"#8C8D8E\",\n              width: \"100%\",\n              borderWidth: 1,\n              height: 0,\n            },\n            b: {\n              color: \"#4C5058\",\n              fontSize: 14,\n              fontWeight: \"bold\",\n              lineHeight: 33,\n            },\n            per: {\n              color: \"#fff\",\n              backgroundColor: \"#4C5058\",\n              padding: [3, 4],\n              borderRadius: 4,\n            },\n          },\n        },\n        series: [\n          {\n            name: \" \",\n            type: \"pie\",\n            radius: \"50%\",\n            data: this.dataArr,\n            itemStyle: {\n              borderWidth: 1,\n              borderColor: \"#fff\",\n            },\n          },\n        ],\n      };\n      myChart.setOption(option, true);\n      window.addEventListener(\"resize\", () => {\n        myChart.resize();\n      });\n    },\n  },\n\n  watch: {\n    data() {\n      this.drawLine()\n    },\n    language() {\n      this.drawLine()\n    },\n  }\n}\n</script>\n\n\n"]}]}